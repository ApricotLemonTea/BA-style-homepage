{
  "rewrites": [
    {
      "source": "/client/v4/:path*",
      "destination": "https://api.cloudflare.com/client/v4/:path*"
    }
  ],
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).(js|css)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}