{
  "rewrites": [
    {
      "source": "/client/v4/:path*",
      "destination": "https://api.cloudflare.com/client/v4/:path*"
    }
  ],
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}